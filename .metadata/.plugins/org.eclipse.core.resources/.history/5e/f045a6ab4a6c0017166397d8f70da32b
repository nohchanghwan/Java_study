import account.Account;

public class AccountMain {

	public static void main(String[] args) throws Exception {

		Account ac = new Account("441-0290-1203",500000,0.073);
		
		System.out.println("계좌정보:"+ac.toString());
		try{
			ac.deposit(-10);
			}catch(Exception e){
			}finally{
			}
		try{
			ac.withdrow(600000);
			}catch(Exception e){
				e.printStackTrace();
			}finally{
			}
		System.out.println("이자"+ac.getInteresRate());

	}

}
