package board.model.vo;

public class BoardVO {

	private int seq	;
	private String title , content , writer , regate ;
	private int viewcnt ;
	
	public BoardVO() {
		super();
	}
	public BoardVO(int seq, String title, String content, String writer, String regate, int viewwcnt) {
		super();
		this.seq = seq;
		this.title = title;
		this.content = content;
		this.writer = writer;
		this.regate = regate;
		this.viewcnt = viewwcnt;
	}
	public int getSeq() {
		return seq;
	}
	public void setSeq(int seq) {
		this.seq = seq;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getWriter() {
		return writer;
	}
	public void setWriter(String writer) {
		this.writer = writer;
	}
	public String getRegate() {
		return regate;
	}
	public void setRegate(String regate) {
		this.regate = regate;
	}
	public int getViewwcnt() {
		return viewcnt;
	}
	public void setViewwcnt(int viewwcnt) {
		this.viewcnt = viewwcnt;
	}
	
	@Override
	public String toString() {
		return "seq=" + seq + ", title=" + title + ", content=" + content + ", writer=" + writer + ", regate="
				+ regate + ", viewwcnt=" + viewcnt;
	}
	
}
