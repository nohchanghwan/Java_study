package com.emp;

	public abstract class Employee {
		
	private	String		name ;
	private int 		number ;
	private	String		department ;
	private int 		salart ;

	public Employee() {
		super();
	}

	public Employee(String name, int number, String department, int salart) {
		super();
		this.name = name;
		this.number = number;
		this.department = department;
		this.salart = salart;
	}
	
	public abstract double tax();

}
